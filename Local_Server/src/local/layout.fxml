<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.text.Font?>

<VBox xmlns:fx="http://javafx.com/fxml"
	fx:controller="local.Controller"
	fillWidth="false"
	alignment="CENTER"
	prefWidth="800" prefHeight="460">
	<children>
		<Label fx:id="time_label" alignment="CENTER" prefWidth="750" text="시간">
			<font>
				<Font size="24.0" />
			</font>
		</Label>
		<Label fx:id="notice_env" alignment="CENTER" prefWidth="750">
			<font>
				<Font size="18.0" />
			</font>
		</Label>
		<Label fx:id="notice_db" alignment="CENTER" prefWidth="750">
			<font>
				<Font size="24.0" />
			</font>
		</Label>
	</children>
</VBox>